{
    "name": "mvnrsa/laravel-sluggable-trait",
    "description": "A trait you can apply to Eloquent models to have slugs automatically generated on save. A slight omprovement on martinbean/laravel-sluggable-trait to use underscore (or any other character) instead of dash in slugs.",
    "version": "0.3.1",
    "type": "library",
    "keywords": ["laravel", "eloquent"],
    "license": "MIT",
    "authors": [
        {
            "name": "Marnus van Niekerk",
            "email": "laravel@mjvn.net"
        }
    ],
    "support": {
        "issues": "https://github.com/mvnrsa/laravel-sluggable-trait/issues",
        "source": "https://github.com/mvnrsa/laravel-sluggable-trait"
    },
    "require": {
        "php": ">=5.4.0",
        "illuminate/database": ">=4.0",
        "illuminate/support": ">=4.0"
    },
    "autoload": {
        "psr-4": {
            "mvnrsa\\Database\\Eloquent\\": "src/"
        }
    }
}
